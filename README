chmod +x setup.sh start.sh stop.sh

# Setup project
./setup.sh
nano backend/.env  # thÃªm GEMINI_API_KEY

# Run application
# Open a terminal for backend
cd backend
source .venv/bin/activate
python cli.py server local
 # Open another terminal for worker
cd backend
source .venv/bin/activate
python cli.py worker local

# Open another terminal for frontend
cd frontend
npm run dev
